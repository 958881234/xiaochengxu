.container{
  height: 100vh;overflow: hidden;
  background-size: contain;
  background-repeat: no-repeat; 
  background-image: url("data:image/png;base64,");
}
.header{
  width: 200rpx;height: 200rpx;border-radius: 50%;
  border: 2px solid red;background-color: #fff;
  margin: 120rpx auto;
  display: block;
}
.loginPane{
  width: 500rpx;
  height: 260rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
}

.loginPane .formItem{
  flex: 1;/* 每个子盒子，分得父盒子设定的高度 */
  display: flex;
  margin:20rpx 0;
}
.password,
.account{
  border-bottom: 1px solid tomato;
  color: tomato;
  flex: 1;
  margin-left: 20rpx;
}
/* 一左一右 */
.spaceBetween{
  justify-content: space-between;
}
.loginBtn,
.registerBtn{
  width: 100%;height: 80rpx;
  border-radius: 40rpx;line-height: 80rpx;
}




